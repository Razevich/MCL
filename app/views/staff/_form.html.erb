<%= form_for(@staff) do |f| %>
<p id="notice" class="notice"><%= notice %></p>

  <div class="modal_box">

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_area :title %>
  </div>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_area :name %>
  </div>
  <div class="field">
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </div>
  <div class="field">
    <%= f.label :img_url %><br>
    <%= f.text_field :img_url %>
  </div>
  <div class="actions">
    <%= f.submit "Save", class: "waves-effect waves-light btn modal-trigger" %>
  </div>
  </div>
<% end %>


